{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "operator-linebreak": "off",
    "global-require": 0,
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": [1, { "html": "ignore" }]
  }
}
