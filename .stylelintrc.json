{
  "extends": "stylelint-config-recommended",
  "ignoreFiles": ["*.svg"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["extends", "tailwind", "screen"]
      }
    ],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "%", "px", "vw", "fr"]
  }
}
